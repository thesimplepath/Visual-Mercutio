<?xml version="1.0" encoding="UTF-8"?>

  <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"  "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">
  <web-app>
  <display-name>utilityServices</display-name>

  <servlet>
    <servlet-name>Logon</servlet-name>
    <servlet-class>com.ibm.services.portal.servlets.LogonServlet</servlet-class>    
  </servlet>

  <servlet>
    <servlet-name>Registration</servlet-name> 
    <servlet-class>com.ibm.services.portal.servlets.RegistrationServlet</servlet-class>
  </servlet>

  <servlet>
     <servlet-name>CreateUserContract</servlet-name> 
     <servlet-class>com.ibm.services.portal.servlets.CreateUserContractServlet</servlet-class>
  </servlet>

  <servlet>
     <servlet-name>DeleteUserContract</servlet-name> 
     <servlet-class>com.ibm.services.portal.servlets.DeleteUserContractServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ActivateUserContract</servlet-name> 
    <servlet-class>com.ibm.services.portal.servlets.ActivateUserContractServlet</servlet-class>
  </servlet>

 <servlet>
       <servlet-name>CreateProviderContract</servlet-name> 
       <servlet-class>com.ibm.services.portal.servlets.CreateProviderContractServlet</servlet-class>
  </servlet>

 <servlet>
       <servlet-name>DeleteProviderContract</servlet-name> 
       <servlet-class>com.ibm.services.portal.servlets.DeleteProviderContractServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ActivateProviderContract</servlet-name> 
    <servlet-class>com.ibm.services.portal.servlets.ActivateProviderContractServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ChangeUser</servlet-name> 
    <servlet-class>com.ibm.services.portal.servlets.ChangeUserServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Reset</servlet-name> 
    <servlet-class>com.ibm.services.portal.servlets.ResetServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>AddressBookServiceServlet</servlet-name>
    <servlet-class>com.ibm.services.portal.servlets.AddressBookServiceServlet</servlet-class>    
  </servlet>

  <servlet-mapping>
    <servlet-name>Logon</servlet-name>
    <url-pattern>/Logon</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Registration</servlet-name>
    <url-pattern>/Registration</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CreateUserContract</servlet-name>
    <url-pattern>/CreateUserContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
     <servlet-name>DeleteUserContract</servlet-name>
    <url-pattern>/DeleteUserContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ActivateUserContract</servlet-name>
    <url-pattern>/ActivateUserContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CreateProviderContract</servlet-name>
    <url-pattern>/CreateProviderContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DeleteProviderContract</servlet-name>
    <url-pattern>/DeleteProviderContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ActivateProviderContract</servlet-name>
    <url-pattern>/ActivateProviderContract</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ChangeUser</servlet-name>
    <url-pattern>/ChangeUser</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Reset</servlet-name>
    <url-pattern>/Reset</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddressBookServiceServlet</servlet-name>
    <url-pattern>/AddressBookServiceServlet</url-pattern>
  </servlet-mapping>

  <mime-mapping>
    <extension>wsdl</extension>
    <mime-type>text/xml</mime-type>
  </mime-mapping>

  <mime-mapping>
    <extension>xsd</extension>
    <mime-type>text/xml</mime-type>
  </mime-mapping>

</web-app>
